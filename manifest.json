{
	"name": "Web Stack",
	"version": "0.1",
	"description": "add a PIP viewer to any web page to remain on top of the current window",
	// "devtools_page": "devtools.html",

	"background": {
		"scripts": [
			"background.js"
		]
	},
	
	"permissions": [
		"tabs", 
		"http://*/*",
		"https://*/*"
	],
	
	"browser_action": {
		// "default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	
	"manifest_version": 2
}
